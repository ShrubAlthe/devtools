{
  "name": "electron-vue3-admin",
  "version": "1.0.0",
  "description": "Electron + Vue3 + Vite + Element Plus 管理系统",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "npx cross-env electron .",
    "electron:dev": "npx cross-env NODE_ENV=development electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "element-plus": "^2.5.0",
    "sharp": "^0.34.4",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^8.2.0",
    "cross-env": "^10.1.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.example.electron-vue3-admin",
    "productName": "Electron Vue3 Admin",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
